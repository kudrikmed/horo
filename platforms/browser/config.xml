<?xml version='1.0' encoding='utf-8'?>
<widget id="com.astropro.horo" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AstroPro</name>
    <description>
        Astrology app with everyday forecast based on zodiac signs
    </description>
    <author email="kudrikmed@gmail.com" href="https://astrohoro.site">
        Dzmitry Kudrytski
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
</widget>
